{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "jsxFactory": "",
    "jsxFragmentFactory": "",
    "noImplicitAny": false,
    "baseUrl": "./src",
    "paths": {
      "components/*": ["components/*"],
      "pages/*": ["pages/*"],
      "API/*": ["API/*"],
      "schemes/*": ["Schemes/*"]
    }
  },
  "include": ["./src/**/*.ts", "src/index.tsx", "src/API/GetListNews.tsx", "src/API/GetItemNews.tsx"]
}
